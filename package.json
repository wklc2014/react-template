{
  "name": "react-template",
  "version": "1.0.0",
  "description": "this is a template for react project",
  "main": "index.js",
  "scripts": {
    "clean": "./node_modules/.bin/rimraf dist/*",
    "dev": "./node_modules/.bin/cross-env NODE_ENV=development nodemon --watch config --exec \"node ./config/devServer.js\"",
    "prod": "./node_modules/.bin/cross-env NODE_ENV=production webpack -p --config config/webpack.prod.config.js",
    "start": "npm run clean && npm run dev",
    "build": "npm run clean && npm run prod"
  },
  "keywords": [
    "react",
    "template",
    "redux",
    "react-router"
  ],
